{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**",
        "${workspaceFolder}/node_modules/**"
      ],
      "program": "${workspaceFolder}/bld_test/run_jasmine.js",
      "outFiles": [
        "{workspaceFolder}/bld_test/**/*.js",
        "{workspaceFolder}/bld/**/*.js"
      ],
      "smartStep": true,
      "sourceMaps": true,
      "outputCapture": "std"
    }
  ]
}